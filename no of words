#include <iostream>
#include<string.h>
using namespace std;
int main() {
    // Write C++ code here
    string str="a b c e  f d";
    int len=str.size();
    int count=1;
    for(int i=0;i<len;i++)
    {
        if((str[i]==' ')&&(str[i+1]!=' '))
        {
            count++;
        }
    }
    cout<<count;
    return 0;
}
